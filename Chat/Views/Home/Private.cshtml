@model IEnumerable<Chat>
<div class="chat-body">
    @{
        var userid = User.FindFirst(System.Security.Claims.ClaimTypes.NameIdentifier).Value;
    }
    @foreach (var item in Model)
    {
        <a class="room-button" asp-controller="Home" asp-action="Chat" asp-route-id="@item.Id">

            @item.Users.FirstOrDefault(c=>c.UserId !=userid).User.UserName
        </a>
    }



</div>
